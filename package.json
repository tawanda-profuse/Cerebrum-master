    "scripts": {
        "start-main": "cross-env NODE_ENV=production node server.js",
        "start-api": "cross-env NODE_ENV=production node workspace/server.js",
        "start-client": "cd client && npm run start-dev",
        "start-all": "concurrently \"node server.js\" \"node workspace/server.js\" \"npm run start-client-wait\"",
        "start-client-wait": "wait-on http://localhost:8000 && npm run start-client",
        "f": "npx prettier . --write",
        "docker-build-main": "docker build --no-cache -t yeduaizw/main-server:latest -f Dockerfile .",
        "docker-build-api": "docker build --no-cache -t yeduaizw/api-server:latest -f workspace/Dockerfile .",
        "docker-push-main": "docker push yeduaizw/main-server:latest",
        "docker-push-api": "docker push yeduaizw/api-server:latest",
        "deploy": "npm run docker-build-main && npm run docker-build-api && npm run docker-push-main && npm run docker-push-api",
        "main-install": "npm install",
        "workspace-install": "cd workspace && npm install",
        "client-install": "cd client && npm install",
        "global-install": "concurrently \"npm run main-install\" \"npm run workspace-install\" \"npm run client-install\""
